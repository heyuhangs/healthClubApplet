page {
  /*height: 100%;*/
}

.container {
  justify-content: initial;
}

.top-edit-btn {
  position: fixed;
  top: 70rpx;
  z-index: 9999999;
  left: 20rpx;
  font-size: 30rpx;
  color: #b5272d;
}
.iphoneCon .top-edit-btn{
  top:108rpx;
}
.title-box {
  margin-top: 25%;
  font-size: 28rpx;
  text-align: center;
  color: #999;
  margin-bottom: 20%;
}

.box-none {
  width: 200rpx;
  height: 200rpx;
  display: block;
  margin: 0 auto;
}

.list-top {
  width: 100%;
  height: 88rpx;
  line-height: 88rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  padding: 0 30rpx;
  font-size: 28rpx;
  align-items: center;
}

.list-top .label {
  color: #000;
}

.list-top .edit-btn {
  color: #999;
  height: 100%;
}

.goodsList {
  width: 100%;
  background-color: #fff;
  margin-bottom: 20rpx;
}

.a-gooods {
  position: relative;
  width: 100%;
  overflow: hidden;
  border-bottom: 1px solid #fbfbfb
}

.a-goods-conts {
  display: flex;
  position: relative;
  justify-content: space-between;
  padding-left: 30rpx;
  width: 870rpx;
  box-sizing: border-box;
}

.a-gooods .bjicon{
  display: inline-block;
  position: absolute;
  z-index: 10;
  top: 33%;
  left: 7px;
  width: 70rpx;
  height: 70rpx;
  transition: margin-left 0.2s ease-in-out;
  background: url("data:image/png;base64,") no-repeat 38rpx center;
  background-size: 38rpx auto;
  background-position: center center;
}

.a-gooods .active {
  background: url("data:image/png;base64,") no-repeat 38rpx center;
  background-size: 38rpx auto;
  background-position: center center;
}

.goods-info {
  display: flex;
  justify-content: space-between;
  padding: 30rpx 0 30rpx 70rpx;
  width: 720rpx;
  box-sizing: border-box;
}

.goods-info .img-box {
  width: 160rpx;
  height: 160rpx;
  overflow: hidden;
  margin-right: 20rpx;
  background-color: #f5f5f5;
}

.goods-info .text-box {
  width: 440rpx;
  position: relative;
}

.goods-info .text-box .goods-title {
  font-size: 28rpx;
  color: #000;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  padding: 10rpx 20rpx 5rpx 0;
}

.goods-info .text-box .goods-label {
  font-size: 26rpx;
  color: #999;
  height: 38rpx;
  line-height: 38rpx;
  margin: 8rpx 0 25rpx 0;
}

.goods-info .text-box .goods-price {
  font-size: 26rpx;
  color: #b5272d;
}

.goods-info .text-box .buy-num {
  width: 164rpx;
  height: 48rpx;
  line-height: 48rpx;
  position: absolute;
  right: 30rpx;
  bottom: 0;
  display: flex;
  /*justify-content: space-between;*/
  font-size: 24rpx;
  text-align: center;
}

.goods-info .text-box .buy-num .jian-btn {
  width: 48rpx;
  height: 100%;
  border-left: 1rpx solid #ccc;
  border-bottom: 1rpx solid #ccc;
  border-top: 1rpx solid #ccc;
  border-bottom-left-radius: 6rpx;
  border-top-left-radius: 6rpx;
}

.goods-info .text-box .buy-num .jian-btn.disabled {
  background-color: #f5f5f9;
  border-left: 1rpx solid #eee;
  border-bottom: 1rpx solid #eee;
  border-top: 1rpx solid #eee;
  color: #ccc;
}

.goods-info .text-box .buy-num .jia-btn {
  width: 48rpx;
  height: 100%;
  border-right: 1rpx solid #ccc;
  border-bottom: 1rpx solid #ccc;
  border-top: 1rpx solid #ccc;
  border-bottom-right-radius: 6rpx;
  border-top-right-radius: 6rpx;
}

.goods-info .text-box .buy-num .jia-btn.disabled {
  background-color: #f5f5f9;
  border-right: 1rpx solid #eee;
  border-bottom: 1rpx solid #eee;
  border-top: 1rpx solid #eee;
  color: #ccc;
}

.goods-info .text-box .buy-num input {
  width: 68rpx;
  height: 48rpx;
  min-height: 48rpx;
  text-align: center;
  font-size: 24rpx;
  border: 1rpx solid #ccc;
}

.goods-info .img-box .img {
  width: 160rpx;
  height: 160rpx;
}

.a-goods-conts .delete-btn {
  position: absolute;
  right: 18%;
  top: 7.5%;
  background-image: url("data:image/png;base64,");
  background-size: cover;
  width: 38rpx;
  height: 38rpx;
  background-repeat: no-repeat;
  background-position: center center;
}

.jiesuan-box {
  display: flex;
  justify-content: space-between;
  width: 100%;
  height: 100rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  border-bottom: 1px solid #f5f5f5;
  background-color: #fff;
  z-index: 1;
  box-shadow: 0px -2px 10px rgba(0, 0, 0, 0.1);
}

.jiesuan-box .to-pay-btn {
  width: 240rpx;
  text-align: center;
  line-height: 100rpx;
  background-color: #b5272d;
  font-size: 32rpx;
  color: #fff;
}

.jiesuan-box .to-pay-btn.no-select {
  background-color: #ccc;
}

.jiesuan-box  .left-price {
  display: flex;
  width: 510rpx;
  justify-content: space-between;
  line-height: 100rpx;
  padding: 0 30rpx 0 0;
  font-size: 28rpx;
  box-sizing: border-box;
}

.jiesuan-box .all-selected {
  padding-left: 90rpx;
  color: #000;
  background: url("data:image/png;base64,") no-repeat 38rpx center;
  background-size: 38rpx auto;
}

.jiesuan-box .all-selected.active {
  background: url("data:image/png;base64,") no-repeat 38rpx center;
  background-size: 38rpx auto;
}

.jiesuan-box .total {
  color: #b5272d;
}

.tips {
  width: 100%;
  text-align: center;
  margin-top: 15px;
  font-size: 30rpx;
  color: #bbb;
  font-weight: 300;
}
.i-modal-btn-ok {
  color: red !important;
}
