/* pages/register/register.wxss */
page{
    /*background: #f2f2f2;*/
}
.header-container {
    width: 100%;
    position: static !important;
    align-items: center;
    background-image: url(https://cdn.it120.cc/apifactory/2018/03/07/fb8857533553034ee28c92bd0dd89b15.jpg);
    background-position: 0px 0px;
    background-size: 100% 100%;
}

.header-container > .header-name {
    font-size: 16px;
    color: #fff;
}
.main{
        margin-top: 5%;
}
.zhu{
    color: #FF7F00;
    font-size: 24rpx;
    width: 90%;
    margin: 2px auto;
    line-height: 32rpx;
}
.phoneInp{
    display: inline-block;
    width: 90% !important;
}
.main .formbtn {
    background-color: rgba(175, 156, 112,1) !important;
    border-color: rgba(175, 156, 112,1) !important;
    color: #fff !important;
    margin-top: 25%;
}
/*.tel #shadow-root{*/
/*    width: 50%;*/
/*}*/
.errmsg{
    display: inline-block;
    box-sizing: border-box;
    font-size: 12px;
    color: red;
    padding-left: 20%;
}
.tel{
    display: flex;
    justify-content: space-between;
    flex-flow: row;
}
.tel i-input{
    width: 65% !important;
}
.tel i-button{
    width: 23% !important;
    margin-right: 6%;
}
.tel .telInp{
    width: 85%;
}
.tel .telBtn{
    box-sizing: border-box;
    width: 100%;
    height: 35px;
    padding: 0 !important;
    line-height: 35px;
    background-color: rgba(175, 156, 112,1) !important;
    border-color: rgba(175, 156, 112,1) !important;
    border-radius: 10px;
}
